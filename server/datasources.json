{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "userService": {
    "name": "userService",
    "connector": "rest",
    "debug": "false",
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "http://localhost:3000/api/SITUsers/{userId}/accessTokens/{token}",
          "headers": {
            "accepts": "application/json",
            "content-type": "application/json",
            "Host": "user.docker.localhost"
          }
        },
        "functions": {
          "checkAuth": [
            "userId",
            "token"
          ]
        }
      }
    ]
  },
  "gridfs": {
    "host": "localhost",
    "port": 27017,
    "database": "mongo-db1",
    "name": "gridfs",
    "connector": "loopback-component-storage-gridfs"
  }
}
